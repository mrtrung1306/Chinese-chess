
@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .box{
        max-width: 1064px; /* Độ rộng của textbox */
        height: 650px; /* Chiều cao của textbox */      
        background-color:aliceblue;
        margin-top:40px;
    }

    .scrollable-textbox{
        width: 532px; /* Chiều rộng của mỗi khung, sẽ hiển thị 2 khung cùng một lúc */
        height:650px;
        float: left; /* Hiển thị các khung theo chiều ngang */
        box-sizing: border-box; /* Bao gồm kích thước của padding và border vào chiều rộng */
        /*padding: 20px; /* Khoảng cách nội dung bên trong khung */
        border: 1px solid #ccc5b9; /* Viền xung quanh khung */
        overflow: auto; /* Kích hoạt thanh cuộn khi nội dung vượt quá kích thước của khung chứa */

    }

    .containere {
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .textbox-frame{
        border: 2px solid #90e0ef; /* Màu và độ dày của khung bên ngoài */
        padding: 20px; /* Khoảng cách giữa khung bên ngoài và textbox */
        background-color: #90e0ef;
    }

    .dropbtn {
        background-color: #fff;
        color: #000;
        padding: 5px;
        font-size: 16px;
        border: none;
        cursor: pointer;
    }

    .dropdown {
        position: relative;
        display: inline-block;
    }

    /* Style cho danh sách sổ xuống */
    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        z-index: 1;
    }

    /* Hiển thị danh sách sổ xuống khi hover hoặc click vào button */
    .dropdown:hover .dropdown-content {
        display: block;
    }
    .item{
        background-color: #fff;
        border:none;
        padding: 5px;
        font-size: 16px;
        color:#000;
    }

    .item-2{
        /*padding-left:50px;*/
    }
    .btn-top{
        display: inline-block;
    }

</style>
