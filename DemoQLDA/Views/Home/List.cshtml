    <div class="textbox-frame">
        <div class="btn-top">
            <button class="item">Mở bàn mới</button>
            <div class="dropdown">
                <button class="dropbtn">Chọn một mục</button>
                <div class="dropdown-content">
                    <a href="#">Mục 1</a>
                    <a href="#">Mục 2</a>
                    <a href="#">Mục 3</a>
                </div>
            </div>
            <button class="item" style="margin-left:50px">Các bàn chơi</button>
            <button class="item" style="background-color:#00b4d8">Còn nữa</button>
        </div>
